{
  "version": 2,
  "builds": [
    {
      "src": "api/index.ts",
      "use": "@vercel/node"
    }
  ],
  "routes": [
    {
      "src": "/(.*)",
      "dest": "/api"
    }
  ],
  "env": {
    "DD_TRACE_ENABLED": "false"
  },
  "crons": [
    {
      "path": "/products/weekly-export/printer",
      "schedule": "0 9 * * 5"
    }
  ]
}
